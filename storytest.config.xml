<suiteConfig>

    <fitSharp.Machine.Application.Settings>
        <inputFolder>document\fitSharp</inputFolder>
        <outputFolder>site</outputFolder>
    </fitSharp.Machine.Application.Settings>

    <fitSharp.Fit.Application.FileExclusions>
        <add>^Sample</add>
    </fitSharp.Fit.Application.FileExclusions>
    
    <ApplicationUnderTest>
        <addNamespace>fit.Test.Acceptance</addNamespace>
        <addNamespace>fit.Test.Double</addNamespace>
        <addNamespace>fit.Runner</addNamespace>
        <addNamespace>fitSharp.Fit.Runner</addNamespace>
        <addNamespace>fitSharp.Fit.Model</addNamespace>
        <addNamespace>fitlibrary</addNamespace>
        <addNamespace>fitSharp.Test.Acceptance.Slim</addNamespace>
        <addAssembly>build\debug\fit.dll</addAssembly>
        <addAssembly>build\debug\fittest.dll</addAssembly>
    </ApplicationUnderTest>
    
    <fit.Service.Service>
        <addOperator>fit.Operators.ExecuteParse</addOperator>
    </fit.Service.Service>
    
</suiteConfig>
